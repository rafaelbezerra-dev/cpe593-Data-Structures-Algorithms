import java.io.FileNotFoundException;
import java.io.FileReader;
import java.util.Calendar;
import java.util.HashSet;
import java.util.Random;
import java.util.Scanner;

public class Quicksort {
	private static void swap(int[] x, int first, int second) {
		x[first] = x[first] + x[second];
		x[second] = x[first] - x[second];
		x[first] = x[first] - x[second];
	}

	public static void quicksort(int[] x, int left, int right) {
		if (right <= left)
			return;

		if ((right - left) == 1) {
			if (x[left] > x[right])
				swap(x, left, right);
			return;
		}

		Random rnd = new Random(Calendar.getInstance().getTimeInMillis());
		int pivotIndex = (Math.abs(rnd.nextInt()) % (right - left + 1)) + left;
		int pivot = x[pivotIndex];
		// int pivot = 6;
		int i = left, j = right;

		while (i < j) {
			while (x[i] < pivot)
				i++;
			while (x[j] > pivot)
				j--;
			if (i < j)
				swap(x, i, j);
		}

		if (left < j)
			quicksort(x, left, j);
		if (right > i)
			quicksort(x, i, right);

	}

	public static void main(String[] args) throws FileNotFoundException {

		Random rnd = new Random(Calendar.getInstance().getTimeInMillis());
		final int n = 100;
		int[] x = new int[n];
		HashSet<Integer> set = new HashSet<>();
		while(set.size() < n){
			set.add((Math.abs(rnd.nextInt()) % 999));			
		}
//		for (int i = 0; i < n; i++) {
//			x[i] = (Math.abs(rnd.nextInt()) % 999999)+100000;
//			System.out.print(x[i] + " ");
//		}

		System.out.println("");
		System.out.println("");
		// Scanner s = new Scanner(new FileReader("HW2.txt"));
		//
		// int n = s.nextInt();
		// int[] x = new int[n];
		//
		// for (int i = 0; i < n; i++)
		// x[i] = s.nextInt();

		quicksort(x, 0, x.length - 1);

		for (int i = 0; i < x.length; i++)
			System.out.print(x[i] + " ");
		// s.close();

		System.out.println("");

	}
}
